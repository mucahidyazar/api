{
  "openapi": "3.1.0",
  "info": {
    "title": "My API",
    "version": "1.0.0",
    "description": "API documentation generated using Zod and zod-openapi"
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Local development server"
    }
  ],
  "paths": {
    "/api/v1/auth/sign-in": {
      "post": {
        "operationId": "signIn",
        "description": "Authenticate existing user and return JWT token",
        "tags": [
          "auth"
        ],
        "summary": "Sign in",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "description": "User email",
                    "example": "user@example.com"
                  },
                  "password": {
                    "type": "string",
                    "minLength": 8,
                    "description": "User password - must be at least 8 characters"
                  }
                },
                "required": [
                  "email",
                  "password"
                ],
                "additionalProperties": false,
                "description": "User sign in data",
                "title": "Sign In"
              },
              "example": {
                "email": "user@example.com",
                "password": "password123"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully authenticated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "description": "Access token",
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6....",
                      "format": "jwt"
                    }
                  },
                  "required": [
                    "token"
                  ],
                  "description": "Successful sign in response",
                  "title": "Sign In Response"
                },
                "example": {
                  "token": "eyJhbGciOiJIUzI1NiIs...",
                  "user": {
                    "id": "123",
                    "email": "user@example.com"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid request body",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "message": "Invalid email or password format"
                }
              }
            }
          },
          "401": {
            "description": "Authentication failed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "message": "Invalid credentials"
                }
              }
            }
          },
          "429": {
            "description": "Too many requests",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/sign-up": {
      "post": {
        "operationId": "signUp",
        "description": "Create a new user",
        "tags": [
          "auth"
        ],
        "summary": "Sign up",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "description": "User email",
                    "example": "user@example.com"
                  },
                  "password": {
                    "type": "string",
                    "minLength": 8,
                    "description": "User password",
                    "format": "password"
                  }
                },
                "required": [
                  "email",
                  "password"
                ],
                "additionalProperties": false,
                "description": "User sign up data",
                "title": "Sign Up"
              },
              "example": {
                "email": "user@example.com",
                "password": "password123"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully created user",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "description": "Access token",
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6....",
                      "format": "jwt"
                    }
                  },
                  "required": [
                    "token"
                  ],
                  "description": "Successful sign up response",
                  "title": "Sign Up Response"
                },
                "example": {
                  "token": "eyJhbGciOiJIUzI1NiIs...",
                  "user": {
                    "id": "123",
                    "email": "user@example.com",
                    "name": "John Doe"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid request body",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "message": "Invalid email or password format"
                }
              }
            }
          },
          "409": {
            "description": "User already exists",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "message": "User with this email already exists"
                }
              }
            }
          },
          "429": {
            "description": "Too many requests",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "message": "Too many signup attempts. Please try again later."
                }
              }
            }
          }
        }
      }
    }
  }
}