  "paths": {
    "/api/v1/auth/sign-up": {
      "post": {
        "operationId": "signUp",
        "description": "Create a new user account with email and password authentication. Returns a JWT access token that can be used to authenticate future requests.",
        "tags": [
          "Authentication"
        ],
        "summary": "Sign up",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "description": "User email",
                    "example": "user@example.com"
                  },
                  "password": {
                    "type": "string",
                    "minLength": 8,
                    "description": "User password",
                    "format": "password"
                  }
                },
                "required": [
                  "email",
                  "password"
                ],
                "additionalProperties": false,
                "description": "User sign up data",
                "title": "Sign Up"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successfully signed up",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "description": "The success of the response",
                      "example": true
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "token": {
                          "type": "string",
                          "description": "Access token",
                          "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6....",
                          "format": "jwt"
                        }
                      },
                      "required": [
                        "token"
                      ],
                      "description": "Successful sign up response",
                      "title": "Sign Up Response"
                    }
                  },
                  "required": [
                    "success",
                    "data"
                  ],
                  "additionalProperties": false,
                  "description": "The response of the API"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request body",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "description": "The success of the response",
                      "example": false
                    },
                    "error": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "description": "The type of the error",
                          "example": "validation_error"
                        },
                        "code": {
                          "type": "string",
                          "description": "The code of the error",
