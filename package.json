{
  "name": "api",
  "license": "MIT",
  "version": "1.0.0",
  "description": "API repository containing various projects including an appointment, stock control, link preview and URL shortener services.",
  "main": "index.js",
  "author": {
    "name": "Mucahid Yazar",
    "email": "mucahidyazar@gmail.com",
    "url": "https://mucahid.dev"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "api",
    "express",
    "service",
    "link-preview",
    "appointment",
    "stock-control"
  ],
  "scripts": {
    "dev": "nodemon --exec ts-node --files -r tsconfig-paths/register ./src/app.ts",
    "dev:node:commit": "nodemon --exec ts-node --files --debug ./src/services/commit/node.ts",
    "dev:node:mhrs": "nodemon --exec ts-node --files --debug ./src/services/mhrs/node.ts",
    "dev:node:stock": "nodemon --exec ts-node --files --debug ./src/services/stock/node.ts",
    "build": "tsc && tsc-alias",
    "start": "node -r module-alias/register ./dist/app.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "test": "jest --coverage",
    "generate:openapi": "ts-node -r module-alias/register ./src/utils/openapi.ts",
    "feed": "ts-node --files -r tsconfig-paths/register ./src/helpers/feed.ts"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/client": "dist/client",
    "@/config": "dist/config",
    "@/constants": "dist/constants",
    "@/controller": "dist/controller",
    "@/data": "dist/data",
    "@/helpers": "dist/helpers",
    "@/middleware": "dist/middleware",
    "@/model": "dist/model",
    "@/requestModel": "dist/model/request",
    "@/responseModel": "dist/model/response",
    "@/routes": "dist/routes",
    "@/services": "dist/services",
    "@/types": "dist/types",
    "@/utils": "dist/utils",
    "@/validation": "dist/validation",
    "@/view": "dist/view"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "dotenv": "16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "2.0.1",
    "module-alias": "^2.2.3",
    "mongoose": "8.9.5",
    "reflect-metadata": "^0.2.2",
    "socket.io": "4.8.1",
    "ts-node": "^10.9.2",
    "winston": "3.17.0",
    "zod": "^3.24.1",
    "zod-openapi": "^4.2.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/jest": "^29.5.14",
    "@types/kill-port": "2.0.3",
    "@types/node": "22.12.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "3.4.2",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
