{
  "name": "api",
  "license": "MIT",
  "version": "1.0.0",
  "description": "API repository containing various projects including an appointment, stock control, link preview and URL shortener services.",
  "main": "index.js",
  "author": {
    "name": "Mucahid Yazar",
    "email": "mucahidyazar@gmail.com",
    "url": "https://mucahid.dev"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "api",
    "express",
    "service",
    "link-preview",
    "appointment",
    "stock-control"
  ],
  "scripts": {
    "dev": "ts-node-dev --files ./src/app.ts",
    "dev:node:commit": "ts-node-dev --files --debug ./src/services/commit/node.ts",
    "dev:node:mhrs": "ts-node-dev --files --debug ./src/services/mhrs/node.ts",
    "dev:node:stock": "ts-node-dev --files --debug ./src/services/stock/node.ts",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "postinstall": "npx puppeteer browsers install chrome",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cheerio": "1.0.0-rc.12",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "jsonfile": "6.1.0",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "2.0.1",
    "moment": "2.30.1",
    "mongoose": "8.5.1",
    "nanoid": "^3.0.0",
    "node-telegram-bot-api": "0.66.0",
    "puppeteer": "22.13.0",
    "simple-git": "3.25.0",
    "socket.io": "4.7.5",
    "typescript": "5.5.3",
    "winston": "3.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonfile": "6.1.4",
    "@types/kill-port": "2.0.3",
    "@types/node": "20.14.10",
    "@types/node-telegram-bot-api": "0.64.7",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "9.7.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-unused-imports": "4.0.0",
    "husky": "9.0.11",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "superagent": "^9.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-node-dev": "2.0.0"
  }
}
