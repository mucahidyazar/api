{
  "name": "api",
  "license": "MIT",
  "version": "1.0.0",
  "description": "API repository containing various projects including an appointment, stock control, link preview and URL shortener services.",
  "main": "index.js",
  "author": {
    "name": "Mucahid Yazar",
    "email": "mucahidyazar@gmail.com",
    "url": "https://mucahid.dev"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "api",
    "express",
    "service",
    "link-preview",
    "appointment",
    "stock-control"
  ],
  "scripts": {
    "dev": "ts-node-dev --files ./src/app.ts",
    "dev:node:commit": "ts-node-dev --files --debug ./src/services/commit/node.ts",
    "dev:node:mhrs": "ts-node-dev --files --debug ./src/services/mhrs/node.ts",
    "dev:node:stock": "ts-node-dev --files --debug ./src/services/stock/node.ts",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:copy": "cp -rf ../api-prisma/prisma ./",
    "prisma:bootstrap": "npm run prisma:copy && npm run prisma:generate",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "postinstall": "prisma generate && npx puppeteer browsers install chrome"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^5.5.2",
    "axios": "^1.2.2",
    "chalk": "4.1.2",
    "cheerio": "1.0.0-rc.12",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonfile": "^6.1.0",
    "kill-port": "^2.0.1",
    "moment": "^2.29.4",
    "mongoose": "^6.8.3",
    "nanoid": "^3.0.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.60.0",
    "prompts": "^2.4.2",
    "puppeteer": "^21.7.0",
    "simple-git": "^3.15.1",
    "socket.io": "^4.5.4",
    "typescript": "^4.9.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonfile": "^6.1.1",
    "@types/kill-port": "^2.0.0",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.57.6",
    "@types/prompts": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^7.0.0",
    "prettier": "^3.2.4",
    "prisma": "^5.5.2",
    "ts-node-dev": "^2.0.0"
  }
}
